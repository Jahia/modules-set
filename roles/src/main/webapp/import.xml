<?xml version="1.0" encoding="ISO-8859-1"?>
<content xmlns:jcr="http://www.jcp.org/jcr/1.0"
         xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jnt="http://www.jahia.org/jahia/nt/1.0"
         xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:jmix="http://www.jahia.org/jahia/mix/1.0"
         xmlns:nt="http://www.jcp.org/jcr/nt/1.0">

    <permissions jcr:primaryType="jnt:permissions" >
        <default-workspace jcr:primaryType="jnt:permissionGroup">
            <jcr:all_default jcr:primaryType="jnt:permission">

                <jcr:read_default jcr:primaryType="jnt:permission"/>

                <jcr:write_default jcr:primaryType="jnt:permission">
                    <jcr:modifyProperties_default jcr:primaryType="jnt:permission"/>
                    <jcr:addChildNodes_default jcr:primaryType="jnt:permission"/>
                    <jcr:removeNode_default jcr:primaryType="jnt:permission"/>
                    <jcr:removeChildNodes_default jcr:primaryType="jnt:permission"/>
                </jcr:write_default>
                <jcr:readAccessControl_default jcr:primaryType="jnt:permission"/>
                <jcr:modifyAccessControl_default jcr:primaryType="jnt:permission"/>
                <jcr:lockManagement_default jcr:primaryType="jnt:permission"/>
                <jcr:versionManagement_default jcr:primaryType="jnt:permission"/>
                <jcr:nodeTypeManagement_default jcr:primaryType="jnt:permission"/>
                <jcr:retentionManagement_default jcr:primaryType="jnt:permission"/>
                <jcr:lifecycleManagement_default jcr:primaryType="jnt:permission"/>
            </jcr:all_default>
        </default-workspace>

        <live-workspace jcr:primaryType="jnt:permissionGroup">
            <jcr:all_live jcr:primaryType="jnt:permission">
                <jcr:read_live jcr:primaryType="jnt:permission"/>
                <jcr:write_live jcr:primaryType="jnt:permission">
                    <jcr:modifyProperties_live jcr:primaryType="jnt:permission"/>
                    <jcr:addChildNodes_live jcr:primaryType="jnt:permission"/>
                    <jcr:removeNode_live jcr:primaryType="jnt:permission"/>
                    <jcr:removeChildNodes_live jcr:primaryType="jnt:permission"/>
                </jcr:write_live>
                <jcr:readAccessControl_live jcr:primaryType="jnt:permission"/>
                <jcr:modifyAccessControl_live jcr:primaryType="jnt:permission"/>
                <jcr:lockManagement_live jcr:primaryType="jnt:permission"/>
                <jcr:versionManagement_live jcr:primaryType="jnt:permission"/>
                <jcr:nodeTypeManagement_live jcr:primaryType="jnt:permission"/>
                <jcr:retentionManagement_live jcr:primaryType="jnt:permission"/>
                <jcr:lifecycleManagement_live jcr:primaryType="jnt:permission"/>
            </jcr:all_live>
        </live-workspace>

        <global jcr:primaryType="jnt:permissionGroup">
            <virtual-sites jcr:primaryType="jnt:permission"/>
            <portlets jcr:primaryType="jnt:permission"/>
            <email-settings jcr:primaryType="jnt:permission"/>
            <root-user jcr:primaryType="jnt:permission"/>
            <server-and-cache jcr:primaryType="jnt:permission"/>
            <server-permissions jcr:primaryType="jnt:permission"/>
            <password-policy jcr:primaryType="jnt:permission"/>
            <patches jcr:primaryType="jnt:permission"/>
            <issue-tracking jcr:primaryType="jnt:permission"/>
            <documentation jcr:primaryType="jnt:permission"/>
            <about jcr:primaryType="jnt:permission"/>
            <users jcr:primaryType="jnt:permission"/>
        </global>

        <modes jcr:primaryType="jnt:permissionGroup">
            <studio-mode jcr:primaryType="jnt:permission"/>
            <contribute-mode jcr:primaryType="jnt:permission"/>
            <edit-mode jcr:primaryType="jnt:permission"/>
        </modes>

        <admin jcr:primaryType="jnt:permissionGroup">
            <jahia-administration jcr:primaryType="jnt:permission"/>
            <groups jcr:primaryType="jnt:permission"/>
            <site-languages jcr:primaryType="jnt:permission"/>
            <site-permissions jcr:primaryType="jnt:permission"/>
            <analytics jcr:primaryType="jnt:permission"/>
            <urlmapping jcr:primaryType="jnt:permission"/>
            <templates jcr:primaryType="jnt:permission"/>
        </admin>

        <edit-mode jcr:primaryType="jnt:permissionGroup">
            <analytics jcr:primaryType="jnt:permission"/>
            <categories jcr:primaryType="jnt:permission"/>
            <content jcr:primaryType="jnt:permission"/>
            <create-page jcr:primaryType="jnt:permission"/>
            <layout jcr:primaryType="jnt:permission"/>
            <list-management jcr:primaryType="jnt:permission"/>
            <metadata jcr:primaryType="jnt:permission"/>
            <options jcr:primaryType="jnt:permission"/>
            <rights jcr:primaryType="jnt:permission"/>
            <seo jcr:primaryType="jnt:permission"/>
            <tags jcr:primaryType="jnt:permission"/>
            <usages jcr:primaryType="jnt:permission"/>
            <workflow jcr:primaryType="jnt:permission"/>
            <publication jcr:primaryType="jnt:permission"/>
            <import jcr:primaryType="jnt:permission"/>
        </edit-mode>

        <managers jcr:primaryType="jnt:permissionGroup">
            <category-manager jcr:primaryType="jnt:permission"/>
            <content-manager jcr:primaryType="jnt:permission"/>
            <file-manager jcr:primaryType="jnt:permission"/>
            <portlet-manager jcr:primaryType="jnt:permission"/>
            <remote-publication-manager jcr:primaryType="jnt:permission"/>
            <site-manager jcr:primaryType="jnt:permission"/>
            <tag-manager jcr:primaryType="jnt:permission"/>
        </managers>

        <selector jcr:primaryType="jnt:permissionGroup">
            <categories jcr:primaryType="jnt:permission"/>
            <content jcr:primaryType="jnt:permission"/>
            <create jcr:primaryType="jnt:permission"/>
            <files-and-images jcr:primaryType="jnt:permission"/>
            <latest jcr:primaryType="jnt:permission"/>
            <portlets jcr:primaryType="jnt:permission"/>
            <search jcr:primaryType="jnt:permission"/>
            <sitemap jcr:primaryType="jnt:permission"/>
            <templates jcr:primaryType="jnt:permission"/>
            <workflow jcr:primaryType="jnt:permission"/>
        </selector>

        <toolbar jcr:primaryType="jnt:permissionGroup">
            <deploy-templates jcr:primaryType="jnt:permission"/>
            <edit jcr:primaryType="jnt:permission"/>
            <language-switcher jcr:primaryType="jnt:permission"/>
            <live-mode jcr:primaryType="jnt:permission"/>
            <new jcr:primaryType="jnt:permission"/>
            <preview-mode jcr:primaryType="jnt:permission"/>
            <publication jcr:primaryType="jnt:permission"/>
            <site-switcher jcr:primaryType="jnt:permission"/>
            <studio-mode jcr:primaryType="jnt:permission"/>
            <user jcr:primaryType="jnt:permission"/>
            <workflow jcr:primaryType="jnt:permission"/>
        </toolbar>

        <workflow jcr:primaryType="jnt:permissionGroup">
            <first-review jcr:primaryType="jnt:permission"/>
            <final-review jcr:primaryType="jnt:permission"/>
        </workflow>

        <wysiwyg-editor-toolbar jcr:primaryType="jnt:permissionGroup">
            <full jcr:primaryType="jnt:permission"/>
            <basic jcr:primaryType="jnt:permission"/>
            <light jcr:primaryType="jnt:permission"/>
        </wysiwyg-editor-toolbar>

        <jobs jcr:primaryType="jnt:permissionGroup">
            <view-all-jobs jcr:primaryType="jnt:permission"/>
        </jobs>
    </permissions>

    <roles jcr:primaryType="jnt:roles" >
        <visitor jcr:primaryType="jnt:role"
                 j:assignedPermissions="live-workspace/read"/>

        <editor jcr:primaryType="jnt:role"
                j:assignedPermissions="default-workspace/* modes/edit-mode modes/contribute actions/* edit-mode/categories edit-mode/content edit-mode/create-page edit-mode/list-management edit-mode/metadata edit-mode/options edit-mode/seo edit-mode/tags languages/* managers/file-manager managers/portlet-manager managers/site-manager selector/content selector/create selector/files-and-images selector/portlets selector/search selector/sitemap toolbar/edit toolbar/language-switcher toolbar/live-mode toolbar/new toolbar/preview-mode toolbar/publication toolbar/site-switcher toolbar/user toolbar/workflow wysiwyg-editor-toolbar/basic"/>

        <editor-in-chief jcr:primaryType="jnt:role"
                         j:assignedPermissions="default-workspace/* modes/edit-mode modes/contribute actions/* edit-mode/* languages/* managers/category-manager managers/file-manager managers/portlet-manager managers/site-manager managers/tag-manager managers/workflow-manager selector/* toolbar/edit toolbar/language-switcher toolbar/live-mode toolbar/new toolbar/preview-mode toolbar/publication toolbar/site-switcher toolbar/user toolbar/workflow wysiwyg-editor-toolbar/*"/>

        <contributor     jcr:primaryType="jnt:role"
                         j:assignedPermissions="default-workspace/* modes/contribute-mode actions/* edit-mode/* languages/* managers/category-manager managers/file-manager managers/portlet-manager managers/site-manager managers/tag-manager managers/workflow-manager selector/* toolbar/edit toolbar/language-switcher toolbar/live-mode toolbar/new toolbar/preview-mode toolbar/publication toolbar/site-switcher toolbar/user toolbar/workflow wysiwyg-editor-toolbar/basic"/>

        <publisher jcr:primaryType="jnt:role"
                   j:assignedPermissions="actions/* edit-mode/categories edit-mode/content edit-mode/create-page edit-mode/list-management edit-mode/metadata edit-mode/publication edit-mode/tags edit-mode/template edit-mode/usages edit-mode/workflow languages/* managers/remote-publication-manager managers/site-manager managers/workflow-manager selector/context selector/latest selector/sitemap toolbar/edit toolbar/language-switcher toolbar/live-mode toolbar/preview-mode toolbar/publication toolbar/site-switcher toolbar/user toolbar/workflow wysiwyg-editor-toolbar/basic"/>

        <seo-manager jcr:primaryType="jnt:role"
                     j:assignedPermissions="modes/edit-mode edit-mode/analytics edit-mode/layout edit-mode/metadata edit-mode/seo edit-mode/usages languages/* managers/category-manager managers/site-manager managers/tag-manager selector/files-and-images selector/search selector/sitemap toolbar/edit toolbar/language-switcher toolbar/live-mode toolbar/preview-mode toolbar/publication toolbar/site-switcher toolbar/user toolbar/workflow wysiwyg-editor-toolbar/light"/>

        <site-administrator jcr:primaryType="jnt:role" jcr:nodeTypes="jnt:virtualsite"
                            j:assignedPermissions=""
                            />

        <translator jcr:primaryType="jnt:role"
                    j:assignedPermissions=""/>

        <web-designer jcr:primaryType="jnt:role"
                      j:assignedPermissions=""/>
        <server-content-manager jcr:primaryType="jnt:role"
            j:assignedPermissions="global/virtual-sites global/portlets global/server-and-cache"/>

        <security-advisor jcr:primaryType="jnt:role"
            j:assignedPermissions="global/server-permissions global/password-policy"/>

        <webmaster jcr:primaryType="jnt:role"
            j:assignedPermissions="global/email-settings global/root-user global/patches global/studio-mode"/>

    </roles>

</content>