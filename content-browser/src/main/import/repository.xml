<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
         xmlns:jnt="http://www.jahia.org/jahia/nt/1.0" xmlns:test="http://www.apache.org/jackrabbit/test"
         xmlns:sv="http://www.jcp.org/jcr/sv/1.0" xmlns:jmix="http://www.jahia.org/jahia/mix/1.0"
         xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:rep="internal">
    <templateSets jcr:primaryType="jnt:templateSets">
        <content-browser j:siteType="module" jcr:mixinTypes="jmix:sitemap" jcr:primaryType="jnt:virtualsite"
                         changefreq="monthly" j:installedModules="content-browser" priority="0.5">
            <home jcr:mixinTypes="jmix:sitemap" jcr:primaryType="jnt:page" changefreq="monthly" j:templateNode=""
                  priority="0.5"/>
            <portlets jcr:primaryType="jnt:portletFolder"/>
            <files jcr:primaryType="jnt:folder"/>
            <contents jcr:primaryType="jnt:contentFolder"/>
            <templates jcr:primaryType="jnt:templatesFolder">
                <files jcr:primaryType="jnt:folder">
                    <themes jcr:primaryType="jnt:folder">
                        <default jcr:primaryType="jnt:folder"/>
                    </themes>
                </files>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <base jcr:mixinTypes="jmix:theme" jcr:primaryType="jnt:template" j:view="apps">
                    <folderTemplate jcr:primaryType="jnt:contentTemplate" j:applyOn="jnt:folder jnt:contentFolder"
                                    j:hiddenTemplate="false" j:defaultTemplate="true">
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <facets jcr:primaryType="jnt:facets"
                                    j:bindedComponent="/templateSets/content-browser/templates/base/folderTemplate/pagecontent/mainResourceDisplay">
                                <fieldFacet mincount="1" field="jmix:tagged;j:tags" jcr:primaryType="jnt:fieldFacet"
                                            missing="false" facet="tags" offset="0" limit="100">
                                    <j:translation_en label="Tags" jcr:language="en" jcr:primaryType="jnt:translation"/>
                                </fieldFacet>
                            </facets>
                            <pager pageSize="10" jcr:primaryType="jnt:pager"
                                   j:bindedComponent="/templateSets/content-browser/templates/base/folderTemplate/pagecontent/mainResourceDisplay"
                                   displayPager="true"/>
                            <mainResourceDisplay j:canOrderInContribution="true" jcr:mixinTypes="jmix:contributeMode"
                                                 j:mainResourceView="table" j:canDeleteInContribution="true"
                                                 jcr:primaryType="jnt:mainResourceDisplay"
                                                 j:contributeTypes="jnt:contentFolder jmix:editorialContent"
                                                 j:editableInContribution="true"/>
                        </pagecontent>
                    </folderTemplate>
                    <editContent
                            j:requiredPermissions="/permissions/repository-permissions/jcr:all_default/jcr:write_default"
                            j:requiredMode="contribute" jcr:primaryType="jnt:contentTemplate" j:applyOn="jnt:content"
                            j:hiddenTemplate="true">
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <mainResourceDisplay-table j:mainResourceView="contribute.table"
                                                       jcr:primaryType="jnt:mainResourceDisplay"/>
                            <tabularList jcr:primaryType="jnt:tabularList">
                                <content jcr:primaryType="jnt:contentList">
                                    <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title" jcr:title="Content"
                                                      jcr:primaryType="jnt:translation"/>
                                    <mainResourceDisplay-edit j:mainResourceView="contribute.edit"
                                                              jcr:primaryType="jnt:mainResourceDisplay"/>
                                </content>
                                <metadata jcr:primaryType="jnt:contentList">
                                    <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title" jcr:title="Metadata"
                                                      jcr:primaryType="jnt:translation"/>
                                    <mainResourceDisplay-metadata j:mainResourceView="contribute.editmetadata"
                                                                  jcr:primaryType="jnt:mainResourceDisplay"/>
                                </metadata>
                                <tags jcr:primaryType="jnt:contentList">
                                    <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title" jcr:title="Tags"
                                                      jcr:primaryType="jnt:translation"/>
                                    <tag jcr:primaryType="jnt:tagging"
                                         j:bindedComponent="/templateSets/content-browser/templates/base/editContent/pagecontent/mainResourceDisplay-table">
                                        <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title" jcr:title="Tag"
                                                          jcr:primaryType="jnt:translation"/>
                                    </tag>
                                    <addTag jcr:mixinTypes="jmix:renderable" jcr:primaryType="jnt:tagging"
                                            j:bindedComponent="/templateSets/content-browser/templates/base/editContent/pagecontent/mainResourceDisplay-table"
                                            j:view="addtag">
                                        <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title" jcr:title="Tag"
                                                          jcr:primaryType="jnt:translation"/>
                                    </addTag>
                                </tags>
                                <categories jcr:primaryType="jnt:contentList">
                                    <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                      jcr:title="Classifications" jcr:primaryType="jnt:translation"/>
                                    <category jcr:mixinTypes="jmix:renderable" jcr:primaryType="jnt:categorizing"
                                              j:bindedComponent="/templateSets/content-browser/templates/base/editContent/pagecontent/mainResourceDisplay-table"
                                              j:view="default">
                                        <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                          jcr:title="Category" jcr:primaryType="jnt:translation"/>
                                    </category>
                                    <category-1 jcr:mixinTypes="jmix:renderable" jcr:primaryType="jnt:categorizing"
                                                j:bindedComponent="/templateSets/content-browser/templates/base/editContent/pagecontent/mainResourceDisplay-table"
                                                j:view="addcategory">
                                        <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                          jcr:title="Category" jcr:primaryType="jnt:translation"/>
                                    </category-1>
                                </categories>
                            </tabularList>
                        </pagecontent>
                    </editContent>
                    <viewContent jcr:primaryType="jnt:contentTemplate" j:hiddenTemplate="true" j:applyOn="jnt:content">
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <mainResourceDisplay-table j:mainResourceView="contribute.table"
                                                       jcr:primaryType="jnt:mainResourceDisplay"/>
                            <mainResourceDisplay j:mainResourceView="default"
                                                 jcr:primaryType="jnt:mainResourceDisplay"/>
                        </pagecontent>
                    </viewContent>
                    <fileTemplate jcr:primaryType="jnt:contentTemplate" j:applyOn="jnt:file" j:defaultTemplate="true">
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <mainResourceDisplay-table j:mainResourceView="contribute.table"
                                                       jcr:primaryType="jnt:mainResourceDisplay"/>
                            <mainResourceDisplay j:mainResourceView="detail" jcr:primaryType="jnt:mainResourceDisplay"/>
                        </pagecontent>
                    </fileTemplate>
                    <user jcr:primaryType="jnt:contentTemplate" j:hiddenTemplate="false" j:applyOn="jnt:user"
                          j:defaultTemplate="true">
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <userDetails jcr:mixinTypes="jmix:renderable" jcr:primaryType="jnt:userDetails"
                                         j:view="full"
                                         j:bindedComponent="/templateSets/content-browser/templates/base/user"/>
                        </pagecontent>
                    </user>
                    <user-private
                            j:requiredPermissions="/permissions/repository-permissions/jcr:all_default/jcr:write_default"
                            jcr:primaryType="jnt:contentTemplate" j:applyOn="jnt:user" j:hiddenTemplate="false"
                            j:defaultTemplate="false">
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <tabularList jcr:primaryType="jnt:tabularList">
                                    <my-profile jcr:primaryType="jnt:contentList">
                                        <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                          jcr:title="My Profile" jcr:primaryType="jnt:translation"/>
                                        <userDetails jcr:primaryType="jnt:userDetails"
                                                     j:bindedComponent="/templateSets/content-browser/templates/base/user-private"/>
                                        <row column="3col484" jcr:primaryType="jnt:row"/>
                                    </my-profile>
                                    <my-content jcr:primaryType="jnt:contentList">
                                        <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                          jcr:title="My Content" jcr:primaryType="jnt:translation"/>
                                        <my-pages j:classname="userMyContentList" j:skin="skins.custom" maxItems="10"
                                                  j:criteria="jcr:created"
                                                  jcr:mixinTypes="jmix:renderable jmix:skinnable jmix:customSkin"
                                                  j:type="jnt:page" jcr:primaryType="jnt:userContentRetrieval"
                                                  j:bindedComponent="/templateSets/content-browser/templates/base/user-private"
                                                  j:view="linkList" j:sortDirection="asc">
                                            <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                              jcr:title="My pages" jcr:primaryType="jnt:translation"/>
                                        </my-pages>
                                    </my-content>
                                    <my-tasks jcr:primaryType="jnt:contentList">
                                        <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                          jcr:title="My Tasks" jcr:primaryType="jnt:translation"/>
                                        <currentUserTasks jcr:primaryType="jnt:currentUserTasks"
                                                          j:bindedComponent="/templateSets/content-browser/templates/base/user-private"/>
                                    </my-tasks>
                                    <my-bookmarks jcr:primaryType="jnt:contentList">
                                        <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                          jcr:title="My Bookmarks" jcr:primaryType="jnt:translation"/>
                                        <currentUserBookmarks jcr:primaryType="jnt:currentUserBookmarks"
                                                              j:bindedComponent="/templateSets/content-browser/templates/base/user-private"/>
                                    </my-bookmarks>
                                    <my-portal jcr:primaryType="jnt:contentList">
                                        <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                          jcr:title="My Portal" jcr:primaryType="jnt:translation"/>
                                        <my-portal-logo.png
                                                j:node="/templateSets/templates-system/templates/files/themes/default/my-portal-logo.png"
                                                jcr:primaryType="jnt:imageReference"/>
                                    </my-portal>
                                    <my-connections jcr:primaryType="jnt:contentList">
                                        <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                          jcr:title="My Connections" jcr:primaryType="jnt:translation"/>
                                    </my-connections>
                                    <my-messages jcr:primaryType="jnt:contentList">
                                        <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                          jcr:title="My Messages" jcr:primaryType="jnt:translation"/>
                                    </my-messages>
                                </tabularList>
                            </pagecontent>
                        </pagecontent>
                        <row-col3 j:skin="skins.box2style1" jcr:mixinTypes="jmix:skinnable"
                                  jcr:primaryType="jnt:contentList">
                            <editUserDetails j:title="false" j:twitterID="false" j:lastName="false" j:facebookID="false"
                                             j:organization="false" j:email="false" j:firstName="false"
                                             j:skypeID="false" j:linkedinID="false" preferredLanguage="true"
                                             j:birthDate="false" j:picture="true" age="true" j:function="false"
                                             jcr:primaryType="jnt:editUserDetails"
                                             j:bindedComponent="/templateSets/content-browser/templates/base/user-private"
                                             j:about="false" j:gender="true"/>
                        </row-col3>
                        <row-col1 j:skin="skins.box2style1" jcr:mixinTypes="jmix:skinnable"
                                  jcr:primaryType="jnt:contentList">
                            <editUserDetails j:title="true" j:twitterID="true" j:lastName="true" j:facebookID="true"
                                             j:organization="true" j:email="true" j:firstName="true" j:skypeID="true"
                                             j:linkedinID="true" preferredLanguage="false" j:picture="false"
                                             j:birthDate="true" j:function="true" jcr:primaryType="jnt:editUserDetails"
                                             j:bindedComponent="/templateSets/content-browser/templates/base/user-private"
                                             j:about="true" j:gender="false"/>
                        </row-col1>
                        <row-col2 j:skin="skins.box2style1" jcr:mixinTypes="jmix:skinnable"
                                  jcr:primaryType="jnt:contentList">
                            <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation"/>
                            <edit-visibility j:title="true" j:twitterID="true" j:facebookID="true" j:lastName="true"
                                             j:organization="true" j:email="true" j:firstName="true" j:skypeID="true"
                                             j:linkedinID="true" jcr:mixinTypes="jmix:renderable"
                                             preferredLanguage="true" j:picture="true" j:birthDate="true" age="true"
                                             j:function="true" jcr:primaryType="jnt:editUserDetails" j:view="visibility"
                                             j:bindedComponent="/templateSets/content-browser/templates/base/user-private"
                                             j:about="true" j:gender="true"/>
                        </row-col2>
                    </user-private>
                </base>
            </templates>
        </content-browser>
    </templateSets>
</content>

