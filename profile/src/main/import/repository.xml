<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
         xmlns:jnt="http://www.jahia.org/jahia/nt/1.0" xmlns:test="http://www.apache.org/jackrabbit/test"
         xmlns:sv="http://www.jcp.org/jcr/sv/1.0" xmlns:jmix="http://www.jahia.org/jahia/mix/1.0"
         xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:rep="internal">
    <templateSets jcr:primaryType="jnt:templateSets">
        <profile changefreq="monthly" j:installedModules="profile" j:siteType="module" j:wcagCompliance="true"
                 jcr:mixinTypes="jmix:sitemap" jcr:primaryType="jnt:virtualsite" priority="0.5">
            <home changefreq="monthly" jcr:mixinTypes="jmix:sitemap" jcr:primaryType="jnt:page" priority="0.5"/>
            <portlets jcr:primaryType="jnt:portletFolder"/>
            <files jcr:primaryType="jnt:folder"/>
            <contents jcr:primaryType="jnt:contentFolder">
                <profilepageitems jcr:primaryType="jnt:contentList">
                    <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" jcr:title="profilePageItems"/>
                    <jahia divClass="user-jahia-home" j:requireLoggedUser="false" j:requirePrivilegedUser="true"
                           j:requiredPermissions="" jcr:mixinTypes="jmix:requiredPermissions"
                           jcr:primaryType="jnt:componentLink" targetTemplate="user-jahia-home">
                        <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" jcr:title="Jahia"/>
                    </jahia>
                    <content divClass="user-content" jcr:primaryType="jnt:componentLink" targetTemplate="user-content">
                        <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" jcr:title="Content"/>
                    </content>
                    <settings divClass="user-edit-details" jcr:primaryType="jnt:componentLink"
                                targetTemplate="user-edit-details">
                        <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" jcr:title="Settings"/>
                    </settings>
                    <tasks divClass="user-tasks" j:requireLoggedUser="false" j:requirePrivilegedUser="true"
                           j:requiredPermissions="" jcr:mixinTypes="jmix:requiredPermissions"
                           jcr:primaryType="jnt:componentLink" targetTemplate="user-tasks">
                        <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" jcr:title="Tasks"/>
                    </tasks>
                </profilepageitems>
            </contents>
            <templates jcr:primaryType="jnt:templatesFolder">
                <files jcr:primaryType="jnt:folder">
                    <themes jcr:primaryType="jnt:folder">
                        <default jcr:primaryType="jnt:folder"/>
                    </themes>
                </files>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <base j:view="apps" jcr:mixinTypes="jmix:theme" jcr:primaryType="jnt:template">
                    <user j:applyOn="jnt:user" j:defaultTemplate="true" j:hiddenTemplate="false" j:priority="10"
                          jcr:primaryType="jnt:contentTemplate">
                        <pagecontent j:skin="skins.box6" j:style="blue" jcr:mixinTypes="jmix:skinnable jmix:style"
                                     jcr:primaryType="jnt:contentList">
                            <userDetails j:bindedComponent="/templateSets/profile/templates/base/user" j:view="full"
                                         jcr:mixinTypes="jmix:renderable" jcr:primaryType="jnt:userDetails"/>
                        </pagecontent>
                    </user>
                    <user-base j:hiddenTemplate="false" jcr:primaryType="jnt:template">
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <row column="2col511" jcr:primaryType="jnt:row"/>
                            <pagecontent j:allowedTypes="" jcr:primaryType="jnt:area"/>
                        </pagecontent>
                        <row-col2 jcr:primaryType="jnt:contentList">
                            <contentList j:skin="skins.box6" j:style="blue" jcr:mixinTypes="jmix:skinnable jmix:style"
                                         jcr:primaryType="jnt:contentList">
                                <loginForm j:displayLoginButton="false" j:displayRememberMeButton="false"
                                           jcr:primaryType="jnt:loginForm"/>
                            </contentList>
                        </row-col2>
                        <user-edit-details j:applyOn="jnt:user" j:defaultTemplate="true" j:hiddenTemplate="false"
                                           j:priority="15" j:requirePrivilegedUser="false"
                                           j:requiredPermissions="/permissions/repository-permissions/jcr:all_default/jcr:write_default"
                                           jcr:primaryType="jnt:contentTemplate">
                            <pagecontent j:columns="2" j:view="columns" jcr:mixinTypes="jmix:renderable jmix:columns"
                                         jcr:primaryType="jnt:contentList">
                                <editUserDetails-1 age="true" j:about="false" j:birthDate="false" j:email="false"
                                                   j:facebookID="false" j:firstName="false" j:function="false"
                                                   j:gender="true" j:lastName="false" j:linkedinID="false"
                                                   j:organization="false" j:picture="true" j:skypeID="false"
                                                   j:title="false" j:twitterID="false"
                                                   jcr:primaryType="jnt:editUserDetails" password="true"
                                                   preferredLanguage="true"/>
                                <editUserDetails j:about="true" j:birthDate="true" j:email="true" j:facebookID="true"
                                                 j:firstName="true" j:function="true" j:gender="false" j:lastName="true"
                                                 j:linkedinID="true" j:organization="true" j:picture="false"
                                                 j:skypeID="true" j:title="true" j:twitterID="true"
                                                 jcr:primaryType="jnt:editUserDetails" preferredLanguage="false"/>
                                <pagecontent jcr:primaryType="jnt:contentList"/>
                            </pagecontent>
                            <row-col3 j:skin="skins.box2style1" jcr:mixinTypes="jmix:skinnable"
                                      jcr:primaryType="jnt:contentList"/>
                            <row-col1 j:skin="skins.box6" j:style="blue" jcr:mixinTypes="jmix:style jmix:skinnable"
                                      jcr:primaryType="jnt:contentList">
                                <editUserDetails-1 age="true" j:about="false"
                                                   j:bindedComponent="/templateSets/profile/templates/base/user-base/user-edit-details"
                                                   j:birthDate="false" j:email="false" j:facebookID="false"
                                                   j:firstName="false" j:function="false" j:gender="true"
                                                   j:lastName="false" j:linkedinID="false" j:organization="false"
                                                   j:picture="true" j:skypeID="false" j:title="false"
                                                   j:twitterID="false" jcr:primaryType="jnt:editUserDetails"
                                                   password="true" preferredLanguage="true"/>
                            </row-col1>
                            <row-col2 j:skin="skins.box7" j:style="green" jcr:mixinTypes="jmix:skinnable jmix:style"
                                      jcr:primaryType="jnt:contentList">
                                <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                  jcr:primaryType="jnt:translation" jcr:title="Public fields"/>
                                <j:translation_fr jcr:language="fr" jcr:mixinTypes="mix:title"
                                                  jcr:primaryType="jnt:translation" jcr:title="Champs public"/>
                            </row-col2>
                        </user-edit-details>
                        <user-tasks j:applyOn="jnt:user" j:defaultTemplate="false" j:hiddenTemplate="false"
                                    j:requireLoggedUser="false" j:requirePrivilegedUser="true"
                                    j:requiredPermissions="/permissions/repository-permissions/jcr:all_default/jcr:write_default"
                                    jcr:primaryType="jnt:contentTemplate">
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <currentUserTasks jcr:primaryType="jnt:currentUserTasks"/>
                            </pagecontent>
                        </user-tasks>
                        <user-jahia-home j:applyOn="jnt:user" j:defaultTemplate="true" j:hiddenTemplate="false"
                                         j:priority="20" j:requireLoggedUser="false" j:requirePrivilegedUser="true"
                                         j:requiredPermissions="/permissions/repository-permissions/jcr:all_default/jcr:write_default"
                                         jcr:primaryType="jnt:contentTemplate">
                            <pagecontent j:columns="2" j:view="columns" jcr:mixinTypes="jmix:renderable jmix:columns"
                                         jcr:primaryType="jnt:contentList">
                                <websites-list-in-edit-mode jcr:primaryType="jnt:listSites" type="edit">
                                    <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                      jcr:primaryType="jnt:translation"
                                                      jcr:title="websites list in edit mode"/>
                                </websites-list-in-edit-mode>
                                <websites-list-in-contribute-mode jcr:primaryType="jnt:listSites" type="contribute">
                                    <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                      jcr:primaryType="jnt:translation"
                                                      jcr:title="websites list in contribute mode"/>
                                </websites-list-in-contribute-mode>
                                <gotoManager jcr:primaryType="jnt:gotoManager" type="document"/>
                                <gotoManager-3 jcr:primaryType="jnt:gotoManager" type="content"/>
                                <gotoStudio jcr:primaryType="jnt:gotoStudio">
                                    <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                      jcr:primaryType="jnt:translation"/>
                                </gotoStudio>
                                <gotoAdmin jcr:primaryType="jnt:gotoAdmin"/>
                                <gotoManager-1 jcr:primaryType="jnt:gotoManager" type="united content"/>
                                <gotoManager-2 jcr:primaryType="jnt:gotoManager" type="roles"/>
                            </pagecontent>
                        </user-jahia-home>
                        <user-content j:applyOn="jnt:user" j:defaultTemplate="false" j:hiddenTemplate="false"
                                      j:requireLoggedUser="false" j:requirePrivilegedUser="false"
                                      j:requiredPermissions="/permissions/repository-permissions/jcr:all_default/jcr:write_default"
                                      jcr:primaryType="jnt:contentTemplate">
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <my-pages j:bindedComponent="" j:classname="userMyContentList" j:criteria="jcr:created"
                                          j:skin="skins.custom" j:sortDirection="asc" j:type="jnt:page"
                                          j:view="linkList"
                                          jcr:mixinTypes="jmix:renderable jmix:skinnable jmix:customSkin"
                                          jcr:primaryType="jnt:userContentRetrieval" maxItems="10">
                                    <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                      jcr:primaryType="jnt:translation" jcr:title="My pages"/>
                                </my-pages>
                            </pagecontent>
                        </user-content>
                        <row-col1 j:skin="skins.box6" j:style="blue" jcr:mixinTypes="jmix:style jmix:skinnable"
                                  jcr:primaryType="jnt:contentList">
                            <profilepageitems j:node="/templateSets/profile/contents/profilepageitems"
                                              jcr:primaryType="jnt:contentReference"/>
                        </row-col1>
                    </user-base>
                </base>
            </templates>
        </profile>
    </templateSets>
</content>

