<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:jnt="http://www.jahia.org/jahia/nt/1.0" xmlns:test="http://www.apache.org/jackrabbit/test" xmlns:sv="http://www.jcp.org/jcr/sv/1.0" xmlns:jmix="http://www.jahia.org/jahia/mix/1.0" xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:rep="internal">
    <templateSets jcr:primaryType="jnt:templateSets">
        <profile changefreq="monthly" j:installedModules="profile" j:siteType="module" j:wcagCompliance="true"
                 jcr:mixinTypes="jmix:sitemap" jcr:primaryType="jnt:virtualsite" priority="0.5">
            <home changefreq="monthly" jcr:mixinTypes="jmix:sitemap" jcr:primaryType="jnt:page" priority="0.5"/>
            <portlets jcr:primaryType="jnt:portletFolder"/>
            <files jcr:primaryType="jnt:folder"/>
            <contents jcr:primaryType="jnt:contentFolder"/>
            <templates jcr:primaryType="jnt:templatesFolder">
                <files jcr:primaryType="jnt:folder">
                    <themes jcr:primaryType="jnt:folder">
                        <default jcr:primaryType="jnt:folder"/>
                    </themes>
                </files>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <base j:view="apps" jcr:mixinTypes="jmix:theme" jcr:primaryType="jnt:template">
                    <user j:applyOn="jnt:user" j:defaultTemplate="true" j:hiddenTemplate="false" j:priority="10"
                          jcr:primaryType="jnt:contentTemplate">
                        <pagecontent j:skin="skins.box6" j:style="blue" jcr:mixinTypes="jmix:skinnable jmix:style"
                                     jcr:primaryType="jnt:contentList">
                            <userDetails j:bindedComponent="/templateSets/profile/templates/base/user" j:view="full"
                                         jcr:mixinTypes="jmix:renderable" jcr:primaryType="jnt:userDetails"/>
                        </pagecontent>
                    </user>
                    <user-privileged j:applyOn="jnt:user" j:defaultTemplate="true" j:hiddenTemplate="false"
                                     j:priority="20" j:requirePrivilegedUser="true"
                                     j:requiredPermissions="/permissions/repository-permissions/jcr:all_default/jcr:write_default"
                                     jcr:primaryType="jnt:contentTemplate">
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <userDetails j:bindedComponent="/templateSets/profile/templates/base/user-privileged"
                                         jcr:primaryType="jnt:userDetails"/>
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <tabularList jcr:primaryType="jnt:tabularList">
                                    <my-profile jcr:primaryType="jnt:contentList">
                                        <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                          jcr:primaryType="jnt:translation" jcr:title="My Profile"/>
                                        <row column="2col124" jcr:primaryType="jnt:row"/>
                                    </my-profile>
                                    <my-content jcr:primaryType="jnt:contentList">
                                        <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                          jcr:primaryType="jnt:translation" jcr:title="My Content"/>
                                        <my-pages
                                                j:bindedComponent="/templateSets/profile/templates/base/user-privileged"
                                                j:classname="userMyContentList" j:criteria="jcr:created"
                                                j:skin="skins.custom" j:sortDirection="asc" j:type="jnt:page"
                                                j:view="linkList"
                                                jcr:mixinTypes="jmix:renderable jmix:skinnable jmix:customSkin"
                                                jcr:primaryType="jnt:userContentRetrieval" maxItems="10">
                                            <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                              jcr:primaryType="jnt:translation" jcr:title="My pages"/>
                                        </my-pages>
                                    </my-content>
                                    <my-tasks jcr:primaryType="jnt:contentList">
                                        <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                          jcr:primaryType="jnt:translation" jcr:title="My Tasks"/>
                                        <currentUserTasks
                                                j:bindedComponent="/templateSets/profile/templates/base/user-privileged"
                                                jcr:primaryType="jnt:currentUserTasks"/>
                                    </my-tasks>
                                    <my-bookmarks jcr:primaryType="jnt:contentList">
                                        <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                          jcr:primaryType="jnt:translation" jcr:title="My Bookmarks"/>
                                        <currentUserBookmarks
                                                j:bindedComponent="/templateSets/profile/templates/base/user-privileged"
                                                jcr:primaryType="jnt:currentUserBookmarks"/>
                                    </my-bookmarks>
                                    <my-portal jcr:primaryType="jnt:contentList">
                                        <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                          jcr:primaryType="jnt:translation" jcr:title="My Portal"/>
                                        <my-portal-logo.png jcr:primaryType="jnt:imageReference"/>
                                        <currentUserPortal
                                                j:bindedComponent="/templateSets/profile/templates/base/user-privileged"
                                                jcr:primaryType="jnt:currentUserPortal"/>
                                    </my-portal>
                                    <my-connections jcr:primaryType="jnt:contentList">
                                        <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                          jcr:primaryType="jnt:translation" jcr:title="My Connections"/>
                                        <currentUserConnections
                                                j:bindedComponent="/templateSets/profile/templates/base/user-privileged"
                                                jcr:primaryType="jnt:currentUserConnections"/>
                                    </my-connections>
                                    <my-messages jcr:primaryType="jnt:contentList">
                                        <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                          jcr:primaryType="jnt:translation" jcr:title="My Messages"/>
                                        <currentUserMessages
                                                j:bindedComponent="/templateSets/profile/templates/base/user-privileged"
                                                jcr:primaryType="jnt:currentUserMessages"/>
                                    </my-messages>
                                </tabularList>
                            </pagecontent>
                        </pagecontent>
                        <row-col3 j:skin="skins.box2style1" jcr:mixinTypes="jmix:skinnable"
                                  jcr:primaryType="jnt:contentList"/>
                        <row-col1 j:skin="skins.box6" j:style="blue" jcr:mixinTypes="jmix:skinnable jmix:style"
                                  jcr:primaryType="jnt:contentList">
                            <editUserDetails-1 age="true" j:about="false"
                                               j:bindedComponent="/templateSets/profile/templates/base/user-privileged"
                                               j:birthDate="false" j:email="false" j:facebookID="false"
                                               j:firstName="false" j:function="false" j:gender="true" j:lastName="false"
                                               j:linkedinID="false" j:organization="false" j:picture="true"
                                               j:skypeID="false" j:title="false" j:twitterID="false"
                                               jcr:primaryType="jnt:editUserDetails" password="true"
                                               preferredLanguage="true"/>
                            <editUserDetails j:about="true"
                                             j:bindedComponent="/templateSets/profile/templates/base/user-privileged"
                                             j:birthDate="true" j:email="true" j:facebookID="true" j:firstName="true"
                                             j:function="true" j:gender="false" j:lastName="true" j:linkedinID="true"
                                             j:organization="true" j:picture="false" j:skypeID="true" j:title="true"
                                             j:twitterID="true" jcr:primaryType="jnt:editUserDetails"
                                             preferredLanguage="false"/>
                        </row-col1>
                        <row-col2 j:skin="skins.box7" j:style="green" jcr:mixinTypes="jmix:skinnable jmix:style"
                                  jcr:primaryType="jnt:contentList">
                            <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                              jcr:primaryType="jnt:translation" jcr:title="Public fields"/>
                            <j:translation_fr jcr:language="fr" jcr:mixinTypes="mix:title"
                                              jcr:primaryType="jnt:translation" jcr:title="Champs public"/>
                            <edit-visibility age="true" j:about="true"
                                             j:bindedComponent="/templateSets/profile/templates/base/user-privileged"
                                             j:birthDate="true" j:email="true" j:facebookID="true" j:firstName="true"
                                             j:function="true" j:gender="true" j:lastName="true" j:linkedinID="true"
                                             j:organization="true" j:picture="true" j:skypeID="true" j:title="true"
                                             j:twitterID="true" j:view="visibility" jcr:mixinTypes="jmix:renderable"
                                             jcr:primaryType="jnt:editUserDetails" preferredLanguage="true"/>
                        </row-col2>
                    </user-privileged>
                    <user-community j:applyOn="jnt:user" j:defaultTemplate="true" j:hiddenTemplate="false"
                                    j:priority="15" j:requirePrivilegedUser="false"
                                    j:requiredPermissions="/permissions/repository-permissions/jcr:all_default/jcr:write_default"
                                    jcr:primaryType="jnt:contentTemplate">
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <userDetails j:bindedComponent="/templateSets/profile/templates/base/user-community"
                                         jcr:primaryType="jnt:userDetails"/>
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <tabularList jcr:primaryType="jnt:tabularList">
                                    <my-profile jcr:primaryType="jnt:contentList">
                                        <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                          jcr:primaryType="jnt:translation" jcr:title="My Profile"/>
                                        <row column="2col124" jcr:primaryType="jnt:row"/>
                                    </my-profile>
                                    <my-content jcr:primaryType="jnt:contentList">
                                        <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                          jcr:primaryType="jnt:translation" jcr:title="My Content"/>
                                        <my-pages
                                                j:bindedComponent="/templateSets/profile/templates/base/user-community"
                                                j:classname="userMyContentList" j:criteria="jcr:created"
                                                j:skin="skins.custom" j:sortDirection="asc" j:type="jnt:page"
                                                j:view="linkList"
                                                jcr:mixinTypes="jmix:renderable jmix:skinnable jmix:customSkin"
                                                jcr:primaryType="jnt:userContentRetrieval" maxItems="10">
                                            <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                              jcr:primaryType="jnt:translation" jcr:title="My pages"/>
                                        </my-pages>
                                    </my-content>
                                    <my-bookmarks jcr:primaryType="jnt:contentList">
                                        <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                          jcr:primaryType="jnt:translation" jcr:title="My Bookmarks"/>
                                        <currentUserBookmarks
                                                j:bindedComponent="/templateSets/profile/templates/base/user-community"
                                                jcr:primaryType="jnt:currentUserBookmarks"/>
                                    </my-bookmarks>
                                    <my-portal jcr:primaryType="jnt:contentList">
                                        <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                          jcr:primaryType="jnt:translation" jcr:title="My Portal"/>
                                        <my-portal-logo.png jcr:primaryType="jnt:imageReference"/>
                                        <currentUserPortal
                                                j:bindedComponent="/templateSets/profile/templates/base/user-community"
                                                jcr:primaryType="jnt:currentUserPortal"/>
                                    </my-portal>
                                    <my-connections jcr:primaryType="jnt:contentList">
                                        <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                          jcr:primaryType="jnt:translation" jcr:title="My Connections"/>
                                        <currentUserConnections
                                                j:bindedComponent="/templateSets/profile/templates/base/user-community"
                                                jcr:primaryType="jnt:currentUserConnections"/>
                                    </my-connections>
                                    <my-messages jcr:primaryType="jnt:contentList">
                                        <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                          jcr:primaryType="jnt:translation" jcr:title="My Messages"/>
                                        <currentUserMessages
                                                j:bindedComponent="/templateSets/profile/templates/base/user-community"
                                                jcr:primaryType="jnt:currentUserMessages"/>
                                    </my-messages>
                                </tabularList>
                            </pagecontent>
                        </pagecontent>
                        <row-col3 j:skin="skins.box2style1" jcr:mixinTypes="jmix:skinnable"
                                  jcr:primaryType="jnt:contentList"/>
                        <row-col1 j:skin="skins.box6" j:style="blue" jcr:mixinTypes="jmix:style jmix:skinnable"
                                  jcr:primaryType="jnt:contentList">
                            <editUserDetails-1 age="true" j:about="false"
                                               j:bindedComponent="/templateSets/profile/templates/base/user-community"
                                               j:birthDate="false" j:email="false" j:facebookID="false"
                                               j:firstName="false" j:function="false" j:gender="true" j:lastName="false"
                                               j:linkedinID="false" j:organization="false" j:picture="true"
                                               j:skypeID="false" j:title="false" j:twitterID="false"
                                               jcr:primaryType="jnt:editUserDetails" password="true"
                                               preferredLanguage="true"/>
                            <editUserDetails j:about="true"
                                             j:bindedComponent="/templateSets/profile/templates/base/user-community"
                                             j:birthDate="true" j:email="true" j:facebookID="true" j:firstName="true"
                                             j:function="true" j:gender="false" j:lastName="true" j:linkedinID="true"
                                             j:organization="true" j:picture="false" j:skypeID="true" j:title="true"
                                             j:twitterID="true" jcr:primaryType="jnt:editUserDetails"
                                             preferredLanguage="false"/>
                        </row-col1>
                        <row-col2 j:skin="skins.box7" j:style="green" jcr:mixinTypes="jmix:skinnable jmix:style"
                                  jcr:primaryType="jnt:contentList">
                            <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                              jcr:primaryType="jnt:translation" jcr:title="Public fields"/>
                            <j:translation_fr jcr:language="fr" jcr:mixinTypes="mix:title"
                                              jcr:primaryType="jnt:translation" jcr:title="Champs public"/>
                            <edit-visibility age="true" j:about="true"
                                             j:bindedComponent="/templateSets/profile/templates/base/user-community"
                                             j:birthDate="true" j:email="true" j:facebookID="true" j:firstName="true"
                                             j:function="true" j:gender="true" j:lastName="true" j:linkedinID="true"
                                             j:organization="true" j:picture="true" j:skypeID="true" j:title="true"
                                             j:twitterID="true" j:view="visibility" jcr:mixinTypes="jmix:renderable"
                                             jcr:primaryType="jnt:editUserDetails" preferredLanguage="true"/>
                        </row-col2>
                    </user-community>
                </base>
            </templates>
        </profile>
    </templateSets>
</content>

