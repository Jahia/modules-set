<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
         xmlns:jnt="http://www.jahia.org/jahia/nt/1.0" xmlns:test="http://www.apache.org/jackrabbit/test"
         xmlns:sv="http://www.jcp.org/jcr/sv/1.0" xmlns:jmix="http://www.jahia.org/jahia/mix/1.0"
         xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:rep="internal">
    <templateSets jcr:primaryType="jnt:templateSets">
        <profile changefreq="monthly" j:installedModules="profile" j:siteType="module" j:wcagCompliance="true"
                 jcr:mixinTypes="jmix:sitemap" jcr:primaryType="jnt:virtualsite" priority="0.5">
            <portlets jcr:primaryType="jnt:portletFolder"/>
            <files jcr:primaryType="jnt:folder"/>
            <contents jcr:primaryType="jnt:contentFolder">
                <profile-page-items jcr:primaryType="jnt:contentFolder">
                    <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title" jcr:primaryType="jnt:translation"
                                      jcr:title="Profile Page Items"/>
                    <content divClass="user-content" jcr:primaryType="jnt:componentLink" targetTemplate="user-content">
                        <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title" jcr:primaryType="jnt:translation"
                                          jcr:title="Content"/>
                    </content>
                    <settings divClass="user-edit-details" jcr:primaryType="jnt:componentLink"
                              targetTemplate="user-edit-details">
                        <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title" jcr:primaryType="jnt:translation"
                                          jcr:title="Settings"/>
                    </settings>
                    <tasks divClass="user-tasks" j:requireLoggedUser="false" j:requirePrivilegedUser="true"
                           jcr:mixinTypes="jmix:requiredPermissions" jcr:primaryType="jnt:componentLink"
                           targetTemplate="user-tasks">
                        <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title" jcr:primaryType="jnt:translation"
                                          jcr:title="Tasks"/>
                    </tasks>
                </profile-page-items>
            </contents>
            <templates jcr:primaryType="jnt:templatesFolder" j:rootTemplatePath="/base">
                <files jcr:primaryType="jnt:folder">
                    <themes jcr:primaryType="jnt:folder">
                        <default jcr:primaryType="jnt:folder"/>
                    </themes>
                </files>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <user-base j:hiddenTemplate="false" jcr:primaryType="jnt:template">
                    <pagecontent jcr:primaryType="jnt:contentList">
                        <user-base-layout customColumn="3, 10, 3" divID="content" jcr:primaryType="jnt:customRow"
                                          principalColumn="2"/>
                    </pagecontent>
                    <row-col2 jcr:primaryType="jnt:contentList"/>
                    <user-edit-details j:applyOn="jnt:user" j:defaultTemplate="true" j:hiddenTemplate="false"
                                       j:priority="15" j:requirePrivilegedUser="false"
                                       j:requiredPermissions="/permissions/repository-permissions/jcr:all_default/jcr:write_default"
                                       jcr:primaryType="jnt:contentTemplate">
                        <pagecontent j:columns="2" j:view="columns" jcr:mixinTypes="jmix:renderable jmix:columns"
                                     jcr:primaryType="jnt:contentList">
                            <editUserDetails-1 age="true" j:about="false" j:birthDate="false" j:email="false"
                                               j:facebookID="false" j:firstName="false" j:function="false"
                                               j:gender="true" j:lastName="false" j:linkedinID="false"
                                               j:organization="false" j:picture="true" j:skypeID="false" j:title="false"
                                               j:twitterID="false" jcr:primaryType="jnt:editUserDetails" password="true"
                                               preferredLanguage="true"/>
                            <editUserDetails j:about="true" j:birthDate="true" j:email="true" j:facebookID="true"
                                             j:firstName="true" j:function="true" j:gender="false" j:lastName="true"
                                             j:linkedinID="true" j:organization="true" j:picture="false"
                                             j:skypeID="true" j:title="true" j:twitterID="true"
                                             jcr:primaryType="jnt:editUserDetails" preferredLanguage="false"/>
                            <pagecontent jcr:primaryType="jnt:contentList"/>
                        </pagecontent>
                        <row-col3 j:skin="skins.box2style1" jcr:mixinTypes="jmix:skinnable"
                                  jcr:primaryType="jnt:contentList"/>
                        <row-col1 j:skin="skins.box6" j:style="blue" jcr:mixinTypes="jmix:style jmix:skinnable"
                                  jcr:primaryType="jnt:contentList">
                            <editUserDetails-1 age="true" j:about="false"
                                               j:bindedComponent="/templateSets/profile/templates/user-edit-details"
                                               j:birthDate="false" j:email="false" j:facebookID="false"
                                               j:firstName="false" j:function="false" j:gender="true" j:lastName="false"
                                               j:linkedinID="false" j:organization="false" j:picture="true"
                                               j:skypeID="false" j:title="false" j:twitterID="false"
                                               jcr:primaryType="jnt:editUserDetails" password="true"
                                               preferredLanguage="true"/>
                        </row-col1>
                        <row-col2 j:skin="skins.box7" j:style="green" jcr:mixinTypes="jmix:style jmix:skinnable"
                                  jcr:primaryType="jnt:contentList">
                            <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                              jcr:primaryType="jnt:translation" jcr:title="Public fields"/>
                            <j:translation_fr jcr:language="fr" jcr:mixinTypes="mix:title"
                                              jcr:primaryType="jnt:translation" jcr:title="Champs public"/>
                        </row-col2>
                    </user-edit-details>
                    <user-tasks j:applyOn="jnt:user" j:defaultTemplate="true" j:priority="20" j:hiddenTemplate="false"
                                j:requireLoggedUser="false" j:requirePrivilegedUser="true"
                                j:requiredPermissions="/permissions/repository-permissions/jcr:all_default/jcr:write_default"
                                jcr:primaryType="jnt:contentTemplate">
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <currentUserTasks jcr:primaryType="jnt:currentUserTasks" viewUserTasks="true"
                                              viewWorkflowTasks="true"/>
                        </pagecontent>
                    </user-tasks>
                    <user-content j:applyOn="jnt:user" j:defaultTemplate="false" j:hiddenTemplate="false"
                                  j:requireLoggedUser="false" j:requirePrivilegedUser="false"
                                  j:requiredPermissions="/permissions/repository-permissions/jcr:all_default/jcr:write_default"
                                  jcr:primaryType="jnt:contentTemplate">
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <my-pages j:classname="userMyContentList" j:criteria="jcr:created" j:skin="skins.custom"
                                      j:sortDirection="asc" j:type="jnt:page" j:view="linkList"
                                      jcr:mixinTypes="jmix:renderable jmix:customSkin jmix:skinnable"
                                      jcr:primaryType="jnt:userContentRetrieval" maxItems="10">
                                <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                                  jcr:primaryType="jnt:translation" jcr:title="My pages"/>
                            </my-pages>
                        </pagecontent>
                    </user-content>
                    <row-col1 j:skin="skins.box6" j:style="blue" jcr:mixinTypes="jmix:style jmix:skinnable"
                              jcr:primaryType="jnt:contentList"/>
                    <user-content-row-col1 j:style="blue" jcr:mixinTypes="jmix:style"
                                           jcr:primaryType="jnt:contentList"/>
                    <user-base-layout-colContent jcr:primaryType="jnt:contentList">
                        <pagecontent j:allowedTypes="" jcr:primaryType="jnt:area"/>
                    </user-base-layout-colContent>
                    <user-base-layout-col1 jcr:primaryType="jnt:contentList">
                        <contentList j:skin="skins.box7" j:style="grey" jcr:mixinTypes="jmix:style jmix:skinnable"
                                     jcr:primaryType="jnt:contentList">
                            <loginForm j:displayLoginButton="false" j:displayRememberMeButton="false"
                                       jcr:primaryType="jnt:loginForm"/>
                        </contentList>
                        <profile-page-items j:node="/templateSets/profile/contents/profile-page-items"
                                            jcr:primaryType="jnt:contentFolderReference"/>
                        <user-profile-aside-left j:allowedTypes="" jcr:primaryType="jnt:area"/>
                    </user-base-layout-col1>
                    <user-base-layout-col3 jcr:primaryType="jnt:contentList">
                        <admin-links j:view="unordered" jcr:mixinTypes="jmix:renderable jmix:listOrdered"
                                     jcr:primaryType="jnt:contentList">
                            <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation"
                                              jcr:title="admin-links"/>
                            <gotoAdmin j:requireLoggedUser="false" j:requirePrivilegedUser="false"
                                       j:requiredPermissions="/permissions/admin/administrationAccess"
                                       jcr:mixinTypes="jmix:requiredPermissions" jcr:primaryType="jnt:gotoAdmin"/>
                            <gotoStudio j:requireLoggedUser="false" j:requirePrivilegedUser="false"
                                        j:requiredPermissions="/permissions/studioMode/studioModeAccess"
                                        jcr:mixinTypes="jmix:requiredPermissions" jcr:primaryType="jnt:gotoStudio"/>
                            <gotoManager j:requireLoggedUser="false" j:requirePrivilegedUser="false"
                                         j:requiredPermissions="/permissions/managers/contentManager"
                                         jcr:mixinTypes="jmix:requiredPermissions" jcr:primaryType="jnt:gotoManager"
                                         type="united content"/>
                        </admin-links>
                        <websites-list-in-edit-mode contribute="true" edit="true" jcr:primaryType="jnt:listSites"
                                                    live="true">
                            <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title"
                                              jcr:primaryType="jnt:translation" jcr:title="websites list in edit mode"/>
                        </websites-list-in-edit-mode>
                        <user-profile-aside-right j:allowedTypes="" jcr:primaryType="jnt:area"/>
                    </user-base-layout-col3>
                </user-base>
            </templates>
        </profile>
    </templateSets>
</content>

