  <templateSets jcr:primaryType="jnt:templateSets">
    <profile j:siteType="module" j:wcagCompliance="true" jcr:mixinTypes="jmix:sitemap" jcr:primaryType="jnt:virtualsite" changefreq="monthly" j:installedModules="profile" priority="0.5">
