<jnt = 'http://www.jahia.org/jahia/nt/1.0'>
<jmix = 'http://www.jahia.org/jahia/mix/1.0'>

[jmix:thinkBoxComponents] > jmix:droppableContent

[jnt:thinkBoxCreation] > jnt:content, jmix:thinkBoxComponents, mix:title

[jnt:thinkBoxItem] > jnt:content, jmix:thinkBoxComponents, jmix:tagged
